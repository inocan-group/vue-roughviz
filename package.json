{
  "name": "vue-roughviz",
  "version": "0.1.0",
  "description": "Use the fun and informal charting capabilities of Rough-Viz charting in Vue.js",
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "lint": "vue-cli-service lint",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "docs:build": "vuepress build docs",
    "docs:serve": "vuepress dev docs"
  },
  "main": "dist/vue-roughviz.ssr.js",
  "module": "dist/vue-roughviz.esm.js",
  "browser": "dist/vue-roughviz.esm.js",
  "unpkg": "dist/vue-roughviz.min.js",
  "files": [
    "dist/*",
    "vue-roughviz.d.ts",
    "src/**/*.vue"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "@vue/babel-preset-app": "^4.5.6",
    "@vue/cli-plugin-babel": "~4.5.6",
    "@vue/cli-plugin-eslint": "~4.5.6",
    "@vue/cli-plugin-typescript": "~4.5.6",
    "@vue/cli-service": "~4.5.6",
    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vuepress/plugin-register-components": "^1.5.4",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "minimist": "^1.2.5",
    "prettier": "^1.19.1",
    "rollup": "^2.7.3",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "rollup-plugin-vue": "^6.0.0-beta.8",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.12",
    "typescript": "~4.0.3",
    "typescript-transform-paths": "^2.0.1",
    "vitepress": "^0.6.0",
    "vue": "^3.0.0-0",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.5.4",
    "vuepress-plugin-typescript": "^0.3.1"
  },
  "dependencies": {
    "rough-viz": "^1.0.5"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "engines": {
    "node": ">=10"
  },
  "types": "vue-roughviz.d.ts"
}
